<?xml version="1.0" encoding="utf-8"?>
<TableRow
    android:background="@color/white"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:background="@drawable/textview_border"
        android:id="@+id/txtName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="4dp"
        android:padding="5dp"
        android:text="0"
        android:textColor="@android:color/black"  />
    <TextView
        android:background="@drawable/textview_border"
        android:id="@+id/txtPack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="4dp"
        android:padding="5dp"
        android:text="0"
        android:textColor="@android:color/black"  />
    <TextView
        android:background="@drawable/textview_border"
        android:textAlignment="center"
        android:id="@+id/txtStockAwal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="4dp"
        android:padding="5dp"
        android:text="0"
        android:textColor="@android:color/black"  />

    <TextView
        android:id="@+id/txtMasuk"
        android:gravity="center"
        android:background="@drawable/textview_border"
        android:textAlignment="center"
        android:padding="5dp"
        android:text="0"
        android:textColor="@android:color/black"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"/>
    <!--                    TODO-->
    <TableLayout>
        <TableRow>
            <TextView
                android:id="@+id/txtIGD"
                android:background="@drawable/textview_border"
                android:textAlignment="center"
                android:padding="5dp"
                android:text="0"
                android:textColor="@android:color/black"
                android:layout_width="100dp"
                android:layout_height="wrap_content"/>
            <TextView
                android:id="@+id/txtKBS"
                android:background="@drawable/textview_border"
                android:textAlignment="center"
                android:padding="5dp"
                android:text="0"
                android:textColor="@android:color/black"
                android:layout_width="100dp"
                android:layout_height="wrap_content"/>
            <TextView
                android:id="@+id/txtGNG"
                android:background="@drawable/textview_border"
                android:textAlignment="center"
                android:padding="5dp"
                android:text="0"
                android:textColor="@android:color/black"
                android:layout_width="100dp"
                android:layout_height="wrap_content"/>
            <TextView
                android:id="@+id/txtKTK"
                android:background="@drawable/textview_border"
                android:textAlignment="center"
                android:padding="5dp"
                android:text="0"
                android:textColor="@android:color/black"
                android:layout_width="100dp"
                android:layout_height="wrap_content"/>
            <TextView
                android:id="@+id/txtRSUD"
                android:background="@drawable/textview_border"
                android:textAlignment="center"
                android:padding="5dp"
                android:text="0"
                android:textColor="@android:color/black"
                android:layout_width="100dp"
                android:layout_height="wrap_content"/>
            <TextView
                android:id="@+id/txtLAIN"
                android:background="@drawable/textview_border"
                android:textAlignment="center"
                android:padding="5dp"
                android:text="0"
                android:textColor="@android:color/black"
                android:layout_width="100dp"
                android:layout_height="wrap_content"/>
        </TableRow>
    </TableLayout>
    <TextView
        android:id="@+id/txtJumlah"
        android:gravity="center"
        android:background="@drawable/textview_border"
        android:textAlignment="center"
        android:padding="5dp"
        android:text="0"
        android:textColor="@android:color/black"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"/>

    <TextView
        android:id="@+id/txtSisa"
        android:gravity="center"
        android:background="@drawable/textview_border"
        android:textAlignment="center"
        android:padding="5dp"
        android:text="0"
        android:textColor="@android:color/black"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"/>
    <TextView
        android:id="@+id/txtKet"
        android:gravity="center"
        android:background="@drawable/textview_border"
        android:textAlignment="center"
        android:padding="5dp"
        android:text=""
        android:textColor="@android:color/black"
        android:layout_width="50dp"
        android:layout_height="match_parent"/>
</TableRow>